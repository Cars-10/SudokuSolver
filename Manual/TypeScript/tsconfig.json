{
    "compilerOptions": {
      "target": "es5",
      "downlevelIteration": true,
      "module": "commonjs",
      "allowJs": true,
      "outDir": "out",
      "sourceMap": true ,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "typeRoots": ["node_modules/@types"]
    },
  "include": ["*.ts"],
  "exclude": ["node_modules"] 
  }
  